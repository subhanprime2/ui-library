{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@subhanprime2/ui-library": [
        "node_modules/@subhanprime2/ui-library/dist"
      ]
    },
    "target": "es2016",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "module": "ESNext",
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "emitDeclarationOnly": true,
    "declarationDir": "./dist"
  }
}